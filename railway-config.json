{
  "servers": [
    {
      "server": {
    "port": 8080,
    "host": "0.0.0.0"
  },
      "websockets": {
        "path": "/*",
        "maxPayloadLength": 65536,
        "idleTimeout": 240,
        "compression": 1,
        "maxConnections": 0
      }
    }
  ],
  
  "tracker": {
    "maxOffers": 20,
    "announceInterval": 120
  },
  
  "websocketsAccess": {
    "allowOrigins": ["*"],
    "denyEmptyOrigin": false
  }
}