{
  "name": "wt-tracker",
  "description": "High performance WebRTC tracker for peer-to-peer networking",
  "version": "0.0.1",
  "license": "Apache-2.0",
  "author": "Novage",
  "homepage": "https://github.com/Novage/wt-tracker",
  "exports": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "type": "module",
  "sideEffects": false,
  "engines": {
    "node": ">=16.0.0"
  },
  "bin": {
    "wt-tracker": "./bin/wt-tracker"
  },
  "keywords": [
    "p2p",
    "peer-to-peer",
    "webrtc",
    "tracker",
    "websockets",
    "networking",
    "signaling",
    "sdp"
