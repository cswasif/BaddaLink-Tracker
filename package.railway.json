  "scripts": {
    "start": "node ./lib/run-tracker.js railway-config.json",
    "build": "npm run clean && npm run compile",
    "compile": "tsc",
    "clean": "rimraf lib",
    "railway:build": "npm run build",
    "railway:start": "npm start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Novage/wt-tracker.git"
  },
  "dependencies": {
    "debug": "^4.4.1",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v20.52.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/node": "^24.0.7",
    "@types/ws": "^8.18.1",
    "globals": "^16.2.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-mockito": "^2.6.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0"
  }
}